#!/data/data/com.termux/files/usr/bin/bash

# ===== COULEURS =====
RED='\e[31m'
GREEN='\e[32m'
YELLOW='\e[33m'
CYAN='\e[36m'
WHITE='\e[37m'
NC='\e[0m'

UA="Mozilla/5.0 (Windows NT 10.0; Win64; x64)"

# ===== LOGO =====
logo() {
clear
echo -e "${RED}"
cat << "EOF"
‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ïó‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ïó ‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ïó  ‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ïó‚ñà‚ñà‚ïó‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ïó‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ïó‚ñà‚ñà‚ïó   ‚ñà‚ñà‚ïó
‚ñà‚ñà‚ïî‚ïê‚ïê‚ïê‚ïê‚ïù‚ñà‚ñà‚ïî‚ïê‚ïê‚ïê‚ïê‚ïù‚ñà‚ñà‚ïî‚ïê‚ïê‚ïê‚ñà‚ñà‚ïó‚ñà‚ñà‚ïî‚ïê‚ïê‚ïê‚ïê‚ïù‚ñà‚ñà‚ïë‚ñà‚ñà‚ïî‚ïê‚ïê‚ïê‚ïê‚ïù‚ïö‚ïê‚ïê‚ñà‚ñà‚ïî‚ïê‚ïê‚ïù‚ïö‚ñà‚ñà‚ïó ‚ñà‚ñà‚ïî‚ïù
‚ñà‚ñà‚ñà‚ñà‚ñà‚ïó  ‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ïó‚ñà‚ñà‚ïë   ‚ñà‚ñà‚ïë‚ñà‚ñà‚ïë     ‚ñà‚ñà‚ïë‚ñà‚ñà‚ñà‚ñà‚ñà‚ïó     ‚ñà‚ñà‚ïë    ‚ïö‚ñà‚ñà‚ñà‚ñà‚ïî‚ïù
‚ñà‚ñà‚ïî‚ïê‚ïê‚ïù  ‚ïö‚ïê‚ïê‚ïê‚ïê‚ñà‚ñà‚ïë‚ñà‚ñà‚ïë   ‚ñà‚ñà‚ïë‚ñà‚ñà‚ïë     ‚ñà‚ñà‚ïë‚ñà‚ñà‚ïî‚ïê‚ïê‚ïù     ‚ñà‚ñà‚ïë     ‚ïö‚ñà‚ñà‚ïî‚ïù
‚ñà‚ñà‚ïë     ‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ïë‚ïö‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ïî‚ïù‚ïö‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ïó‚ñà‚ñà‚ïë‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ïó   ‚ñà‚ñà‚ïë      ‚ñà‚ñà‚ïë
‚ïö‚ïê‚ïù     ‚ïö‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïù ‚ïö‚ïê‚ïê‚ïê‚ïê‚ïê‚ïù  ‚ïö‚ïê‚ïê‚ïê‚ïê‚ïê‚ïù‚ïö‚ïê‚ïù‚ïö‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïù   ‚ïö‚ïê‚ïù      ‚ïö‚ïê‚ïù
EOF
echo -e "${CYAN}                    ùöÖùü∏${NC}"
}

# ===== USERNAME CHECK (FIX) =====
check_user() {
    site="$1"
    url="$2"

    code=$(curl -A "$UA" -s -o /dev/null -w "%{http_code}" "$url")

    if [[ "$code" == "200" || "$code" == "301" || "$code" == "302" ]]; then
        echo -e "${GREEN}$site : $url${NC}"
    else
        echo -e "${RED}$site : NOT FOUND${NC}"
    fi
}

# ===== MENU =====
menu() {
logo
echo
echo -e "${RED}[${WHITE}1${RED}] ${YELLOW}YOUR IP${NC}          ${RED}[${WHITE}2${RED}] ${YELLOW}DISCORD SERVER${NC}       ${RED}[${WHITE}3${RED}] ${YELLOW}IP LOOKUP${NC}"
echo -e "${RED}[${WHITE}4${RED}] ${YELLOW}DISCORD ID${NC}       ${RED}[${WHITE}5${RED}] ${YELLOW}PHONE LOOKUP${NC}         ${RED}[${WHITE}6${RED}] ${YELLOW}USERNAME${NC}"
echo -e "${RED}[${WHITE}7${RED}] ${YELLOW}EMAIL OSINT${NC}      ${RED}[${WHITE}8${RED}] ${YELLOW}FAKE ID${NC}              ${RED}[${WHITE}0${RED}] ${YELLOW}LEAVE${NC}"
echo

read -p "$(echo -e ${CYAN}'Choix : '${NC})" c

case $c in

1)
clear
echo -e "${GREEN}YOUR IP : $(curl -s https://api.ipify.org)${NC}"
read
;;

2)
termux-open-url https://discord.gg/TEdDqwsaAm
;;

3)
clear
read -p "IP : " ip
curl -s https://ipinfo.io/$ip
read
;;

4)
clear
read -p "Discord ID : " did
DISCORD_EPOCH=1420070400000
timestamp=$(( (did >> 22) + DISCORD_EPOCH ))
date -d @$((timestamp/1000))
read
;;

5)
clear
read -p "Phone (+33) : " phone
echo -e "${YELLOW}====== PHONE LOOKUP ======${NC}"
echo -e "${GREEN}Number  : $phone${NC}"
echo -e "${GREEN}Country : France${NC}"
echo -e "${GREEN}Type    : Mobile / Fixed (Unknown)${NC}"
echo -e "${GREEN}Carrier : Unknown${NC}"
echo -e "${GREEN}Address : Unknown${NC}"
echo -e "${GREEN}Email   : Unknown${NC}"
echo -e "${YELLOW}==========================${NC}"
read
;;

6)
clear
read -p "Username : " u
echo -e "${YELLOW}====== USERNAME OSINT ======${NC}"
check_user "GitHub"     "https://github.com/$u"
check_user "Instagram"  "https://www.instagram.com/$u/"
check_user "Twitter"    "https://twitter.com/$u"
check_user "TikTok"     "https://www.tiktok.com/@$u"
echo -e "${YELLOW}============================${NC}"
read
;;

7)
clear
read -p "Email : " e
domain="${e##*@}"
echo -e "${YELLOW}====== EMAIL OSINT ======${NC}"
echo -e "${GREEN}Email  : $e${NC}"
echo -e "${GREEN}Domain : $domain${NC}"
echo -e "${GREEN}MX :${NC}"
dig mx "$domain" +short 2>/dev/null || echo "Unavailable"
echo -e "${YELLOW}==========================${NC}"
read
;;

8)
clear
read -p "Pr√©nom : " p
read -p "Nom : " n
age=$((RANDOM%9+12))
user="$(echo "$p$n$RANDOM" | tr 'A-Z' 'a-z')"
phone="+33$((RANDOM%900000000+100000000))"

echo -e "${YELLOW}====== FAKE ID ======${NC}"
echo -e "${GREEN}Name     : $p $n${NC}"
echo -e "${GREEN}Age      : $age${NC}"
echo -e "${GREEN}Phone    : $phone${NC}"
echo -e "${GREEN}Username : $user${NC}"
echo -e "${GREEN}Email    : $user@gmail.com${NC}"
echo -e "${YELLOW}======================${NC}"
read
;;

0) exit ;;
*) echo "Invalid"; sleep 1 ;;
esac

menu
}

menu
